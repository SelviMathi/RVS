#include <stdio.h>
#include <malloc.h>

struct node
    {
        int data;
        struct node*next;
    }QUEUE;

typedef struct node*QueuePTR;
QueuePTR newnode,front,rear,tptr;

void enqueue(int num)
{
    newnode = (QueuePTR)malloc(sizeof(QUEUE));
    newnode->data = num;
    newnode->next = NULL;
    if (front == NULL)
            front=rear=newnode;
    else
    {
    rear->next = newnode;
    rear = newnode;
    }
    printf("%d ",num);
}

int dequeue()
{
    int temp;
    if (front==NULL)
           return -1;
    else
        {
        tptr=front;
        temp=front->data;
        front=front->next;
        free(tptr);
        return temp;
        }
}

int main()
{
    int arr[]={10,5,20,57,30};
    int index,noe;
    noe=sizeof(arr)/sizeof(arr[0]);
    for(index=0;index<noe;index++)
    enqueue(arr[index]);
    printf("\nDequeued element:%d",dequeue());
    return 0;
}
