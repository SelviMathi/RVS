#include<stdio.h>
#include<malloc.h>
typedef struct node
{
    int data;
    struct node *next;
}NODE;
NODE *newnode,*tptr,*shadow,*start=NULL;
void insert(int num)
{
newnode=(NODE*)malloc(sizeof(NODE));
newnode->data = num;
newnode->next = NULL;
for(tptr=start;tptr;shadow=tptr,tptr=tptr->next);
    if(tptr==start)
{
    newnode->next=start;
    start=newnode;
}
else
{
    newnode->next=shadow->next;
    shadow->next=newnode;
}
}
int loopofLL()
{
    start->next->next->next=start->next;   
    NODE *fast=start;
    NODE *slow=start;
    while(slow!=NULL && fast !=NULL && fast->next!=NULL)
    {
        slow=slow->next;
        fast=fast->next->next;
    if(slow==fast)
    return 1;
   }
    return 0;
}
int main()
{
insert(20);
insert(10);
insert(30);
insert(5);
insert(50);
if(loopofLL())
printf("\nTrue");
else
printf("\nFalse");
return 0;
}
